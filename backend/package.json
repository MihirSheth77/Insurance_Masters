{
  "name": "insurance-masters-backend",
  "version": "1.0.0",
  "description": "Insurance Masters Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:csv": "jest --testPathPattern=tests/csv",
    "lint": "eslint src/ tests/",
    "seed": "node scripts/seedData.js",
    "import:all": "node scripts/importData/importAll.js",
    "import-data": "node scripts/importData/importAll.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "express-rate-limit": "^6.10.0",
    "bottleneck": "^2.19.5",
    "csv-parser": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.5.0",
    "joi": "^17.9.2",
    "express-validator": "^7.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "mongodb-memory-server": "^8.15.1",
    "nock": "^13.3.3",
    "nodemon": "^3.0.1",
    "eslint": "^8.49.0",
    "@jest/globals": "^29.7.0",
    "jest-environment-node": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/app.js",
      "!src/config/database.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/tests/**/*.spec.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "keywords": ["insurance", "ichra", "healthcare", "api"],
  "author": "Insurance Masters Team",
  "license": "MIT"
} 